  "scoring": {
    "factors": [
      {
        "key": "timeline",
        "name": "Timeline",
        "weight": 0.2,
        "description": "Do they need to sell soon? Within 30 days = pass.",
        "passIf": "Seller indicates urgency \u2014 moving, behind on payments, inherited, divorce, etc."
      },
      {
        "key": "motivation",
        "name": "Motivation",
        "weight": 0.25,
        "description": "Are they distressed or must-sell? High motivation = pass.",
        "passIf": "Distressed situation \u2014 foreclosure, probate, divorce, job relocation, tax issues"
      },
      {
        "key": "price",
        "name": "Price Expectation",
        "weight": 0.2,
        "description": "Is their asking price realistic for a wholesale deal?",
        "passIf": "Asking price leaves room for wholesale spread (at or below 70% ARV minus repairs)"
      },
      {
        "key": "source",
        "name": "Lead Source Quality",
        "weight": 0.15,
        "description": "Did they come to us (inbound) or did we find them (outbound)?",
        "passIf": "Inbound lead (TV, website, referral) OR PPL with strong intent signals"
      },
      {
        "key": "condition",
        "name": "Property Condition",
        "weight": 0.2,
        "description": "Is the property distressed enough for a wholesale deal?",
        "passIf": "Visible distress in Street View or seller mentions major repairs needed"
      }
    ],
